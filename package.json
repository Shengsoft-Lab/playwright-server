  "license": "MIT",
  "dependencies": {
    "playwright": "^1.55.0",
    "http-proxy": "^1.18.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "eslint": "^8.50.0",
    "jest": "^29.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/teaegg/playwright-server.git"
  },
  "bugs": {
    "url": "https://github.com/teaegg/playwright-server/issues"
  },
