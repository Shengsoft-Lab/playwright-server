{
  "name": "playwright-server",
  "version": "1.0.0",
  "description": "A Node.js server for managing Playwright browser instances with on-demand creation, TTL cleanup, and support for WebKit and Chrome browsers",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "keywords": [
    "playwright",
    "browser",
    "automation",
    "webkit",
    "chrome",
    "proxy",
    "websocket"
